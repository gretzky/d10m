{
  "name": "d10m",
  "version": "1.0.5",
  "license": "MIT",
  "author": "Sara Federico <sfed617@gmail.com>",
  "repository": "https://github.com/gretzky/d10m",
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "compile": "babel src -d dist",
    "prepare": "yarn run lint && yarn run compile",
    "precommit": "lint-staged",
    "lint": "standard src/** --fix"
  },
  "dependencies": {
    "react": "^16.3.1",
    "styled-components": "^4.0.3",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "standard": "^12.0.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  }
}
